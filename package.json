{
  "name": "react-shopper",
  "version": "0.1.0",
  "description": "Full-stack ecommerce app made with create-react-app client and Node server",
  "engines": {
    "node": "16.19.0"
  },
  "scripts": {
    "start": "node server",
    "dev:server": "nodemon server",
    "dev:client": "npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "cd client && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "@stripe/stripe-js": "^1.49.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "stripe": "^11.15.0",
    "use-shopping-cart": "^3.1.5"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
